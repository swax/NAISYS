{
  "name": "@naisys/hub",
  "version": "1.0.0",
  "description": "NAISYS Hub - Enables monitoring and communication of between multiple NAISYS agent managers",
  "type": "module",
  "main": "dist/naisysHub.js",
  "exports": {
    ".": "./dist/naisysHub.js",
    "./services/hubSyncServer": "./dist/services/hubSyncServer.js",
    "./services/hubForwardService": "./dist/services/hubForwardService.js",
    "./services/hubServerLog": "./dist/services/hubServerLog.js",
    "./services/hubServer": "./dist/services/hubServer.js"
  },
  "scripts": {
    "clean": "npx rimraf dist",
    "dev": "tsx watch src/naisysHub.ts",
    "start": "node dist/naisysHub.js",
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@naisys/database": "file:../../packages/database",
    "@naisys/hub-protocol": "file:../../packages/hub-protocol",
    "@naisys-supervisor/server": "file:../supervisor/server",
    "commander": "^14.0.0",
    "dotenv": "^17.2.3",
    "pino": "^10.2.0",
    "socket.io": "^4.8.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
