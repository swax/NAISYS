{
  "name": "naisys",
  "description": "Node.js Autonomous Intelligence System",
  "version": "1.7.0",
  "type": "module",
  "main": "dist/naisys.js",
  "preferGlobal": true,
  "bin": {
    "naisys": "bin/naisys"
  },
  "scripts": {
    "compile/run/attachable": "tsc && node --inspect dist/naisys.js ../../agents/assistant.yaml",
    "agent:odoo": "node dist/naisys.js ../../agents/odoo.yaml",
    "agent:assistant": "node dist/naisys.js ../../agents/assistant.yaml",
    "agent:nightwatch": "node dist/naisys.js ../../agents/nightwatch.yaml",
    "agent:scdb:program-director": "cd ../../agents/scdb && node ../../apps/naisys/dist/naisys.js program-director.yaml --overlord",
    "agent:onshape": "node dist/naisys.js ../../agents/onshape.yaml",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "eslint": "npx eslint src",
    "test": "tsc && node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPatterns=dist/__tests__",
    "prettier": "npx prettier --write .",
    "dependency-graph": "madge --image dependency-graph.png dist",
    "detect-cycles": "madge --circular dist",
    "gen-tuning": "tsc && node dist/__fine-tuning__/gen-jsonl.js",
    "npm:publish:dryrun": "npm run clean && npm ci && npm run compile && npm publish --dry-run",
    "postinstall": "node -e \"process.platform !== 'win32' && require('child_process').execSync('chmod +x ./bin/*')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swax/NAISYS.git"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "cli-tools",
    "devops-tools",
    "naisys"
  ],
  "author": "John Marshall",
  "license": "MIT",
  "homepage": "https://naisys.org",
  "devDependencies": {
    "@types/escape-html": "1.0.4",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.10.0",
    "@types/text-table": "0.2.5",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "eslint": "9.39.0",
    "jest": "30.2.0",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.68.0",
    "@google/genai": "1.28.0",
    "@naisys-overlord/server": "file:../overlord/server",
    "@xterm/headless": "5.5.0",
    "chalk": "5.6.2",
    "commander": "14.0.2",
    "dotenv": "17.2.3",
    "escape-html": "1.0.3",
    "js-yaml": "4.1.0",
    "openai": "6.7.0",
    "sharp": "0.34.4",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7",
    "strip-ansi": "7.1.2",
    "text-table": "0.2.0",
    "tiktoken": "1.0.22",
    "tree-kill": "1.2.2",
    "web-streams-polyfill": "4.2.0"
  }
}
