{
  "name": "naisys",
  "description": "Networked Agents Interface System",
  "version": "2.3.0",
  "type": "module",
  "main": "dist/naisys.js",
  "preferGlobal": true,
  "bin": {
    "naisys": "bin/naisys"
  },
  "scripts": {
    "build/run/attachable": "tsc && node --inspect dist/naisys.js ../../agents/assistant.yaml --supervisor",
    "agent:assistant": "node dist/naisys.js ../../agents/assistant.yaml --integrated-hub --supervisor --erp",
    "agent:scdb:program-director": "cd ../../agents/scdb && node ../../apps/naisys/dist/naisys.js program-director.yaml",
    "clean": "rimraf dist",
    "build": "tsc",
    "eslint": "npx eslint src",
    "test": "tsc && node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --testPathPatterns=dist/__tests__",
    "npm:publish:dryrun": "npm run clean && npm run build && npm publish --dry-run",
    "postinstall": "node -e \"process.platform !== 'win32' && require('child_process').execSync('chmod +x ./bin/*')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swax/NAISYS.git"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "cli-tools",
    "devops-tools",
    "naisys"
  ],
  "author": "John Marshall",
  "license": "MIT",
  "homepage": "https://naisys.org",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@naisys/database": "file:../../packages/database",
    "@types/escape-html": "^1.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.3",
    "@types/text-table": "^0.2.5",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@naisys/hub": "file:../hub"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/genai": "^1.40.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@naisys/common": "file:../../packages/common",
    "@naisys/hub-protocol": "file:../../packages/hub-protocol",
    "@xterm/headless": "^6.0.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.2.4",
    "escape-html": "^1.0.3",
    "js-yaml": "^4.1.1",
    "openai": "^6.19.0",
    "sharp": "^0.34.5",
    "socket.io-client": "^4.8.3",
    "string-argv": "^0.3.2",
    "strip-ansi": "^7.1.2",
    "text-table": "^0.2.0",
    "tiktoken": "^1.0.22",
    "tree-kill": "^1.2.2",
    "web-streams-polyfill": "^4.2.0"
  }
}
