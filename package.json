{
  "name": "naisys",
  "description": "Node.js Autonomous Intelligence System",
  "version": "1.6.1",
  "type": "module",
  "main": "dist/naisys.js",
  "preferGlobal": true,
  "bin": {
    "naisys": "bin/naisys"
  },
  "scripts": {
    "compile/run/attachable": "tsc && node --inspect dist/naisys.js ./agents/solo-websites/webdev-fansite.yaml",
    "agent:assistant": "node dist/naisys.js ./agents/assistant.yaml",
    "agent:nightwatch": "node dist/naisys.js ./agents/nightwatch.yaml",
    "agent:scdb:find-sketch": "node dist/naisys.js ./agents/scdb/01-find-sketch.yaml",
    "agent:scdb:sketch-details": "node dist/naisys.js ./agents/scdb/02-sketch-details.yaml",
    "agent:scdb:get-headshots": "node dist/naisys.js ./agents/scdb/03-get-headshots.yaml",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "eslint": "npx eslint src",
    "test": "tsc && node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPatterns=dist/__tests__",
    "prettier": "npx prettier --write .",
    "dependency-graph": "madge --image dependency-graph.png dist",
    "detect-cycles": "madge --circular dist",
    "gen-tuning": "tsc && node dist/__fine-tuning__/gen-jsonl.js",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm install",
    "npm:publish:dryrun": "npm run clean && npm ci && npm run compile && npm publish --dry-run",
    "postinstall": "node -e \"process.platform !== 'win32' && require('child_process').execSync('chmod +x ./bin/*')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swax/NAISYS.git"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "cli-tools",
    "devops-tools",
    "naisys"
  ],
  "author": "John Marshall",
  "license": "MIT",
  "homepage": "https://naisys.org",
  "devDependencies": {
    "@types/escape-html": "1.0.4",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.0.6",
    "@types/text-table": "0.2.5",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "eslint": "9.30.0",
    "jest": "30.0.3",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.55.0",
    "@google/generative-ai": "0.24.1",
    "@xterm/headless": "5.5.0",
    "chalk": "5.4.1",
    "commander": "14.0.0",
    "dotenv": "17.0.0",
    "escape-html": "1.0.3",
    "js-yaml": "4.1.0",
    "openai": "5.8.2",
    "sharp": "0.34.2",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7",
    "strip-ansi": "7.1.0",
    "text-table": "0.2.0",
    "tiktoken": "1.0.21",
    "tree-kill": "1.2.2",
    "web-streams-polyfill": "4.1.0"
  }
}
