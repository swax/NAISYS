{
  "name": "naisys",
  "description": "Node.js Autonomous Intelligence System",
  "version": "1.7.0",
  "type": "module",
  "main": "dist/naisys.js",
  "preferGlobal": true,
  "bin": {
    "naisys": "bin/naisys"
  },
  "scripts": {
    "compile/run/attachable": "tsc && node --inspect dist/naisys.js ./agents/solo-websites/webdev-fansite.yaml",
    "agent:odoo": "node dist/naisys.js ./agents/odoo.yaml",
    "agent:assistant": "node dist/naisys.js ./agents/assistant.yaml",
    "agent:nightwatch": "node dist/naisys.js ./agents/nightwatch.yaml",
    "agent:scdb:program-director": "node dist/naisys.js ./agents/scdb/program-director.yaml",
    "agent:scdb:pipeline-manager": "node dist/naisys.js ./agents/scdb/pipeline-manager.yaml",
    "agent:scdb:find-sketches": "node dist/naisys.js ./agents/scdb/subagents/01-find-sketches.yaml",
    "agent:scdb:sketch-details": "node dist/naisys.js ./agents/scdb/subagents/02-sketch-details.yaml",
    "agent:scdb:get-headshots": "node dist/naisys.js ./agents/scdb/subagents/03-get-headshots.yaml",
    "agent:scdb:update-database": "node dist/naisys.js ./agents/scdb/subagents/04-update-database.yaml",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "eslint": "npx eslint src",
    "test": "tsc && node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPatterns=dist/__tests__",
    "prettier": "npx prettier --write .",
    "dependency-graph": "madge --image dependency-graph.png dist",
    "detect-cycles": "madge --circular dist",
    "gen-tuning": "tsc && node dist/__fine-tuning__/gen-jsonl.js",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm install",
    "npm:publish:dryrun": "npm run clean && npm ci && npm run compile && npm publish --dry-run",
    "postinstall": "node -e \"process.platform !== 'win32' && require('child_process').execSync('chmod +x ./bin/*')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swax/NAISYS.git"
  },
  "keywords": [
    "ai",
    "agent",
    "automation",
    "cli-tools",
    "devops-tools",
    "naisys"
  ],
  "author": "John Marshall",
  "license": "MIT",
  "homepage": "https://naisys.org",
  "devDependencies": {
    "@types/escape-html": "1.0.4",
    "@types/js-yaml": "4.0.9",
    "@types/node": "24.3.0",
    "@types/text-table": "0.2.5",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "eslint": "9.33.0",
    "jest": "30.0.5",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.60.0",
    "@google/generative-ai": "0.24.1",
    "@xterm/headless": "5.5.0",
    "chalk": "5.5.0",
    "commander": "14.0.0",
    "dotenv": "17.2.1",
    "escape-html": "1.0.3",
    "js-yaml": "4.1.0",
    "openai": "5.12.2",
    "sharp": "0.34.3",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7",
    "strip-ansi": "7.1.0",
    "text-table": "0.2.0",
    "tiktoken": "1.0.22",
    "tree-kill": "1.2.2",
    "web-streams-polyfill": "4.1.0"
  }
}
