{
  "name": "naisys",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build & run": "tsc && node dist/main.js",
    "clean:win": "rmdir dist /s /q",
    "eslint": "npx eslint --rulesdir eslint-rules src",
    "prettier": "npx prettier --write .",
    "compile": "tsc",
    "detect-cycles": "madge --circular dist",
    "dependency-graph": "madge --image dependency-graph.png dist",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm update"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "20.11.10",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "eslint": "8.56.0",
    "prettier": "3.2.4",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "dotenv": "16.4.1",
    "js-yaml": "^4.1.0",
    "openai": "4.26.0",
    "tiktoken": "1.0.13"
  }
}
