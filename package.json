{
  "name": "naisys",
  "version": "1.0.0",
  "main": "naisys.js",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "run agent:coder": "node dist/naisys.js ./agents/coder.yaml",
    "run agent:quality": "node dist/naisys.js ./agents/quality.yaml",
    "run agent:manager": "node dist/naisys.js ./agents/manager.yaml",
    "clean:win": "rmdir dist /s /q",
    "eslint": "npx eslint --rulesdir eslint-rules src",
    "prettier": "npx prettier --write .",
    "detect-cycles": "madge --circular dist",
    "dependency-graph": "madge --image dependency-graph.png dist",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u && npm update"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "20.11.19",
    "@types/text-table": "^0.2.5",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "eslint": "8.56.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "chalk": "5.3.0",
    "commander": "^12.0.0",
    "dotenv": "16.4.5",
    "js-yaml": "^4.1.0",
    "openai": "4.28.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "text-table": "^0.2.0",
    "tiktoken": "1.0.13"
  }
}
